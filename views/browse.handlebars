<div class="row">
    <div class="col s4">
      <!--this row is for the search bar  -->
  <div class="row">
    <div class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <input type="text" id="autocomplete-input" class="autocomplete">
          <label for="autocomplete-input">cuisine</label>
        </div>
      </div>
    </div>
  </div>

      <div class="row">
        {{#each recipes}}
        {{#compare this.id "<=" 5}}
        <div class="card ">
          <div class="card-image waves-effect waves-block waves-light">
            <img class="activator" src="{{this.recipe_image}}">
          </div>
          <div class="card-content">
            <span class="card-title activator grey-text text-darken-4">{{this.recipe_name}}<i class="material-icons right">toc</i></span>
            <p><a href="{{this.recipe_url}}">Link</a></p>
          </div>
        <div class="card-reveal">
          <span class="card-title grey-text text-darken-4">{{this.recipe_name}}<i class="material-icons right">close</i></span>
          {{this.instructions}}
        </div>
      </div>
      {{/compare}}
      {{/each}}
    </div>
  </div>

